{
    "type": "commonjs",
    "name": "cheerio-select",
    "description": "CSS selector engine supporting jQuery selectors",
    "version": "2.1.0",
    "author": "Felix Boehm <me@feedic.com>",
    "funding": {
        "url": "https://github.com/sponsors/fb55"
    },
    "license": "BSD-2-Clause",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git://github.com/cheeriojs/cheerio-select.git"
    },
    "directories": {
        "lib": "lib/"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "module": "lib/esm/index.js",
    "exports": {
        "import": "./lib/esm/index.js",
        "require": "./lib/index.js"
    },
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "build": "npm run build:cjs && npm run build:esm",
        "build:cjs": "tsc --sourceRoot https://raw.githubusercontent.com/cheeriojs/cheerio-select/$(git rev-parse HEAD)/src/",
        "build:esm": "npm run build:cjs -- --module esnext --target es2019 --outDir lib/esm && echo '{\"type\":\"module\"}' > lib/esm/package.json",
        "format": "npm run format:es && npm run format:biome",
        "format:biome": "biome check --write .",
        "format:es": "npm run lint:es -- --fix",
        "lint": "npm run lint:es && npm run lint:biome",
        "lint:biome": "biome check .",
        "lint:es": "eslint src",
        "prepare": "npm run build",
        "test": "npm run test:jest && npm run lint",
        "test:jest": "jest"
    },
    "dependencies": {
        "boolbase": "^1.0.0",
        "css-select": "^6.0.0",
        "css-what": "^7.0.0",
        "domelementtype": "^2.3.0",
        "domhandler": "^5.0.3",
        "domutils": "^3.2.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.4.4",
        "@eslint/compat": "^2.0.2",
        "@feedic/eslint-config": "^0.2.2",
        "@types/boolbase": "^1.0.3",
        "@types/jest": "^29.5.14",
        "@types/node": "^25.3.0",
        "eslint": "^10.0.1",
        "eslint-config-prettier": "^10.1.8",
        "htmlparser2": "^10.1.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "coverageProvider": "v8",
        "moduleNameMapper": {
            "^(.*)\\.js$": [
                "$1",
                "$1.js"
            ]
        }
    }
}
